sudo useradd -c "Test User Account" john
grep john /etc/passwd
sudo useradd -e 2025-01-22 john
sudo chage -l john
sudo useradd -r john
useradd -D
sudo useradd -D -s /bin/bash
sudo useradd -D | grep -i shell

